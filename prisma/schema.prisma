generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

enum CafeteriaType {
  haksik
  dodam
  faculty
  dormitory
}

enum MealType {
  breakfast
  lunch
  dinner
}

model Meal {
  id            String        @id @default(cuid())
  cafeteriaType CafeteriaType
  mealType      MealType
  name          String
  menu          Json
  date          DateTime
  createdAt     DateTime      @default(now())
  updatedAt     DateTime      @updatedAt
}
